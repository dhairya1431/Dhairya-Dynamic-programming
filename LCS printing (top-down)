#include<bits/stdc++.h>
using namespace std;
int dp[102][102];
int ans;

int LCS(string x,string y,int n,int m)
{
    if(n==0 || m==0 )
     return 0;
     
     if(dp[n][m]!=-1) return dp[n][m];
     
     LCS(x,y,n-1,m);
     LCS(x,y,n,m-1);
     if(x[n-1]==y[m-1])
     {
         dp[n][m]=1+LCS(x,y,n-1,m-1);
         ans=max(ans,dp[n][m]);
         return ans;
     }
     return dp[n][m]=0;
}
int main()
{
    int t;cin>>t;
    while(t--)
    {
        ans=0;
        memset(dp,-1,sizeof(dp));
        int n,m;cin>>n>>m;
        string x,y;cin>>x>>y;
        LCS(x,y,n,m);
        cout<<ans<<endl;
    }
}
